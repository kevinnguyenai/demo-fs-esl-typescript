{
    "name": "demo-fs-esl-ts",
    "version": "0.0.1",
    "description": "demo typescript app using lb4 with freeswitch esl communication",
    "keywords": [
        "loopback-application",
        "loopback"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "engines": {
        "node": "12 || 14 || 16 || 17"
    },
    "scripts": {
        "build": "lb-tsc",
        "build:watch": "lb-tsc --watch",
        "lint": "yarn run eslint && yarn run prettier:check",
        "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
        "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
        "prettier:check": "yarn run prettier:cli -l",
        "prettier:fix": "yarn run prettier:cli --write",
        "eslint": "lb-eslint --report-unused-disable-directives .",
        "eslint:fix": "yarn run eslint --fix",
        "pretest": "yarn run rebuild",
        "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
        "posttest": "yarn run lint",
        "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
        "docker:build": "docker build -t demo-fs-esl-ts .",
        "docker:run": "docker run -p 3000:3000 -d demo-fs-esl-ts",
        "premigrate": "yarn run build",
        "migrate": "node ./dist/migrate",
        "preopenapi-spec": "yarn run build",
        "openapi-spec": "node ./dist/openapi-spec",
        "prestart": "yarn run rebuild",
        "start": "node -r source-map-support/register .",
        "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
        "rebuild": "yarn run clean && yarn run build",
        "postinstall": "husky install",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/kevinnguyenai/demo-fs-esl-ts.git"
    },
    "author": "kevinnguyenai <kevin.nguyen.ai@gmail.com>",
    "license": "M.I.T",
    "files": [
        "README.md",
        "dist",
        "src",
        "!*/__tests__"
    ],
    "dependencies": {
        "@loopback/authentication": "^9.0.2",
        "@loopback/authentication-jwt": "^0.12.2",
        "@loopback/boot": "^4.0.0",
        "@loopback/core": "^3.0.0",
        "@loopback/health": "^0.11.2",
        "@loopback/repository": "^4.0.0",
        "@loopback/rest": "^12.0.2",
        "@loopback/rest-explorer": "^5.0.2",
        "@loopback/service-proxy": "^4.0.0",
        "async": "^3.2.4",
        "axios": "^0.27.2",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^16.0.1",
        "eventemitter2": "^6.4.7",
        "faker": "^5.5.3",
        "fetch": "^1.1.0",
        "form-data": "^4.0.0",
        "jwt-decode": "^3.1.2",
        "loopback-connector-rest": "^4.0.1",
        "md5": "^2.3.0",
        "modesl": "^1.2.1",
        "multer": "^1.4.5-lts.1",
        "request": "^2.88.2",
        "sha256": "^0.2.0",
        "tslib": "^2.4.0",
        "winston": "^3.8.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-conventional": "^17.0.3",
        "@loopback/build": "^8.0.0",
        "@loopback/eslint-config": "^12.0.0",
        "@loopback/testlab": "^4.0.0",
        "@types/async": "^3.2.15",
        "@types/cli": "^0.11.21",
        "@types/faker": "^5.5.9",
        "@types/md5": "^2.3.2",
        "@types/modesl": "^1.1.2",
        "@types/multer": "^1.4.7",
        "@types/node": "^10.17.60",
        "@types/request": "^2.48.8",
        "@types/sha256": "^0.2.0",
        "@types/xml2js": "^0.4.11",
        "eslint": "^7.32.0",
        "husky": "7.0.0",
        "source-map-support": "^0.5.20",
        "typescript": "~4.5.2"
    }
}
